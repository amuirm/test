#include<stdio.h>
struct date {
	int year;
	int month;
	int day;
};
struct student {
	int number;
	char name[10];
	struct date dob;
	double grade;
};
struct student s1;
int main()
{
	s1.dob.year = 1983;
	s1.dob.month = 03;
	s1.dob.day = 29;
	printf("%d\n",s1.dob.year);
	printf("%02d\n",s1.dob.month);
	printf("%d\n",s1.dob.day);
}
